<h1>ログイン</h1>

<%= form_with url: , method: :post, local: true do |f| %> <!-- FIXIT: urlのパスを設定 -->
  <%= f.label :email, 'メールアドレス' %>
  <%= f.text_field :email %>
  <%= f.label :password, 'パスワード' %>
  <%= f.password_field :password %>
  <%= f.submit 'ログイン' %>
<% end %>
