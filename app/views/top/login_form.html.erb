<h1>ログイン</h1>

<%= form_with url: login_path , method: :post, local: true do |f| %>
  <%= f.label :email, 'メールアドレス' %>
  <%= f.text_field :email %>
  <%= f.label :password, 'パスワード' %>
  <%= f.password_field :password %>
  <%= f.submit 'ログイン' %>
<% end %>
<br>
<%= link_to "アカウント登録", new_user_path %>
<%= link_to "キャンセル", top_main_path %>